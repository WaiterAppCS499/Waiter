<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.app.noemi.waiter">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme" android:name="my.app.noemi.waiter.ParseApplication">
        <activity android:name="my.app.noemi.waiter.HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>
        <activity android:name="my.app.noemi.waiter.WaitingActivity"></activity>
        <activity android:name="my.app.noemi.waiter.SignUpActivity"></activity>
        <activity android:name="my.app.noemi.waiter.AccountSettingsActivity"></activity>
        <activity android:name="my.app.noemi.waiter.UsernameSettingsActivity"></activity>
        <activity android:name="my.app.noemi.waiter.PasswordSettingsActivity"></activity>
        <activity android:name="my.app.noemi.waiter.SearchActivity"></activity>
        <activity android:name="my.app.noemi.waiter.PostActivity"></activity>
        <activity android:name="my.app.noemi.waiter.ResultsActivity"></activity>
        <activity android:name="my.app.noemi.waiter.PostsActivity"></activity>
        <activity android:name="my.app.noemi.waiter.PostedActivity"></activity>
        <activity android:name="my.app.noemi.waiter.NearbyActivity"></activity>
        <activity android:name="my.app.noemi.waiter.FavoritesActivity"></activity>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>
